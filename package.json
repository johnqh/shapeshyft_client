{
  "name": "@sudobility/shapeshyft_client",
  "version": "0.0.31",
  "description": "React client library for ShapeShyft API with TanStack Query hooks",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "bunx tsc -p tsconfig.build.json",
    "build:watch": "bunx tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "bunx eslint src --ext .ts,.tsx",
    "lint:fix": "bunx eslint src --ext .ts,.tsx --fix",
    "format": "bunx prettier --write src/**/*.ts",
    "format:check": "bunx prettier --check src/**/*.ts",
    "typecheck": "bunx tsc --noEmit"
  },
  "keywords": [
    "shapeshyft",
    "llm",
    "api-client",
    "react",
    "tanstack-query",
    "typescript"
  ],
  "author": "John Huang",
  "license": "MIT",
  "peerDependencies": {
    "@sudobility/shapeshyft_types": "^1.0.27",
    "@sudobility/types": "^1.9.44",
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sudobility/di": "^1.5.18",
    "@sudobility/shapeshyft_types": "^1.0.27",
    "@sudobility/types": "^1.9.44",
    "@tanstack/react-query": "^5.90.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.3.0",
    "prettier": "^3.6.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  }
}
